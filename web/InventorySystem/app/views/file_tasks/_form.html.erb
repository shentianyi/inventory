<%= simple_form_for(@file_task) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.input :data_file_id %>
    <%= f.input :err_file_id %>
    <%= f.input :status %>
    <%= f.input :remark %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
